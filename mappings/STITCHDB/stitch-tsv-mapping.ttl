@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rev: <http://purl.org/stuff/rev#> .
@prefix schema: <http://schema.org/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix : <http://tib.de/ontario/mapping#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix stitch: <http://bio2rdf.org/ns/stitch#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .

:stitch_chemical
  	rml:logicalSource [
              rml:source <http://ontario.tib.eu/Federation1/datasource/STITCH-tsv>;
  						rml:referenceFormulation ql:TSV;
              rml:iterator "chemicals.v5.0.tsv"
  						];
  	rr:subjectMap [
  		      rr:template "http://stitch.embl.de/chemical/{chemical}";
        		rr:class stitch:Chemical
  	];
    rr:predicateObjectMap [
      rr:predicate stitch:molecularWeight;
      rr:objectMap [
        rr:template "molecular_weight";
        rr:datatype xsd:string
      ]
    ];
    rr:predicateObjectMap [
      rr:predicate stitch:smilesString;
      rr:objectMap [
        rml:reference "SMILES_string"
      ]
    ];
    rr:predicateObjectMap [
      rr:predicate stitch:id;
      rr:objectMap [
        rml:reference "chemical"
      ]
    ];
    rr:predicateObjectMap [
      rr:predicate stitch:name;
      rr:objectMap [
        rml:reference "name"
      ]
    ].

