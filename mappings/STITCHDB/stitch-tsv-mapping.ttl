:stitch_chemical_chemical_links
  	rml:logicalSource [
              rml:source <http://ontario.tib.eu/Federation1/datasource/STITCH-tsv>;
  						rml:referenceFormulation ql:TSV;
              rml:iterator "chemical_chemical.links.detailed.v5.0.tsv"
  						];
  	rr:subjectMap [
  		      rr:template "http://stitch.embl.de/chemicallink/{chemical1}_{chemical2}";
        		rr:class stitch:ChemicalChemicalLink
  	];
    rr:predicateObjectMap [
      rr:predicate stitch:chemical2;
      rr:objectMap [
         rr:template "http://stitch.embl.de/chemical/{chemical2}";
         rr:class stitch:Chemical
      ]
    ];
    rr:predicateObjectMap [
      rr:predicate stitch:score;
      rr:objectMap [
        rml:reference "combined_score"
      ]
    ];
    rr:predicateObjectMap [
      rr:predicate stitch:chemical1;
      rr:objectMap [
      rr:template "http://stitch.embl.de/chemical/{chemical1}";
      rr:class stitch:Chemical
      ]
    ].

