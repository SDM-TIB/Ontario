@prefix rr: <http://www.w3.org/ns/r2rml#>.
@prefix rml: <http://semweb.mmlab.be/ns/rml#>.
@prefix ql: <http://semweb.mmlab.be/ns/ql#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix rev: <http://purl.org/stuff/rev#> .
@prefix schema: <http://schema.org/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix : <http://tib.eu/ontario/mapping#> .
@prefix iasis: <http://project-iasis.eu/vocab/> .


:Annotation
	rml:logicalSource [ rml:source "Annotation";
						rml:referenceFormulation ql:Cypher;
						rml:iterator "node"
						];
	rr:subjectMap [
		rr:template "http://project-iasis.eu/Annotation/{id}";
		rr:class iasis:Annotation
	];
	rr:predicateObjectMap [
		rr:predicate iasis:annID;
		rr:objectMap [
			rml:reference "id"
		]
	];
	rr:predicateObjectMap [
		rr:predicate iasis:annLabel;
		rr:objectMap [
			rml:reference "label"
		]
	];
	rr:predicateObjectMap [
		rr:predicate iasis:semanticType;
		rr:objectMap [
			rml:reference "semantic_types[*]"
		]
	];
	rr:predicateObjectMap [
		rr:predicate iasis:snomedct_us;
		rr:objectMap [
			rml:reference "SNOMEDCT_US"
		]
	];
	rr:predicateObjectMap [
		rr:predicate iasis:hpo;
		rr:objectMap [
			rml:reference "HPO"
		]
	];
	rr:predicateObjectMap [
		rr:predicate iasis:drugbankId;
		rr:objectMap [
			rml:reference "DRUGBANK"
		]
	];
	rr:predicateObjectMap [
		rr:predicate iasis:hgnc;
		rr:objectMap [
			rml:reference "HGNC"
		]
	];
    rr:predicateObjectMap [
      rr:predicate lsdl:causes;
      rr:objectMap [
        rr:parentTriplesMap :Annotation
      ]
    ];
    rr:predicateObjectMap [
      rr:predicate lsdl:mentionedIn;
      rr:objectMap [
        rr:parentTriplesMap :Publication
      ]
    ];
    rr:predicateObjectMap [
      rr:predicate lsdl:associatedWith;
      rr:objectMap [
        rr:parentTriplesMap :Annotation
      ]
    ];
    rr:predicateObjectMap [
      rr:predicate lsdl:predisposes;
      rr:objectMap [
        rr:parentTriplesMap :Annotation
      ]
    ];
    rr:predicateObjectMap [
      rr:predicate lsdl:stimulates;
      rr:objectMap [
        rr:parentTriplesMap :Annotation
      ]
    ];
    rr:predicateObjectMap [
      rr:predicate lsdl:interactsWith;
      rr:objectMap [
        rr:parentTriplesMap :Annotation
      ]
    ];
    rr:predicateObjectMap [
      rr:predicate lsdl:inhibits;
      rr:objectMap [
        rr:parentTriplesMap :Annotation
      ]
    ].

:Publication
	rml:logicalSource [ rml:source "Publication";
						rml:referenceFormulation ql:Cypher;
						rml:iterator "node"
						];
	rr:subjectMap [
		rr:template "http://project-iasis.eu/Publication/{pmid}";
		rr:class iasis:Publication
	];
	rr:predicateObjectMap [
		rr:predicate iasis:pmid;
		rr:objectMap [
			rml:reference "pmid"
		]
	];
	rr:predicateObjectMap [
		rr:predicate iasis:title;
		rr:objectMap [
			rml:reference "title"
		]
	];
	rr:predicateObjectMap [
		rr:predicate iasis:year;
		rr:objectMap [
			rr:template "year"
		]
    ];
	rr:predicateObjectMap [
		rr:predicate iasis:journal;
		rr:objectMap [
			rr:template "journal"
		]
    ];
	rr:predicateObjectMap [
		rr:predicate iasis:authors;
		rr:objectMap [
			rr:template "authors"
		]
    ];
	rr:predicateObjectMap [
		rr:predicate iasis:hasTopic;
		rr:objectMap [
			rr:parentTriplesMap :Annotation
		]
    ].